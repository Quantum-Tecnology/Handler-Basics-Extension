{
  "name": "quantumtecnology/handler-basics-extension",
  "license": "MIT",
  "homepage": "http://www.gustavosantarosa.esy.es/",
  "description": "Handler Basics Extension",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "sentry/sentry-laravel": "^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "QuantumTecnology\\HandlerBasicsExtension\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Luis Gustavo Santarosa",
      "email": "gustavo-computacao@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "gustavo-computacao@hotmail.com"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "QuantumTecnology\\HandlerBasicsExtension\\Provider\\HandlerBasicsExtensionProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
